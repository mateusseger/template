VITE_APP_AUTHORITY=https://your-sso-provider.com/auth/realms/YourRealm
VITE_APP_RESPONSE_TYPE=id_token token
VITE_APP_SCOPE=openid profile email roles
VITE_APP_CLIENT_ID=your-client-id
VITE_APP_API_URL=https://your-api-url.com
VITE_APP_TOKEN_KEY=app-token

# Development Mode (true = mock authentication)
VITE_DEV_AUTH_BYPASS=true

# Mock User Roles (comma-separated: admin,editor,viewer,user)
VITE_DEV_MOCK_ROLES=admin,user

# Theme Configuration (herval, taqi, iplace)
VITE_APP_THEME=herval
